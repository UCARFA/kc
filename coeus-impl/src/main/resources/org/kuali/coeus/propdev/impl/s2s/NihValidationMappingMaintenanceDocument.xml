  <bean id="NihValidationMappingMaintenanceDocument" parent="NihValidationMappingMaintenanceDocument-parentBean"/>

  <bean id="NihValidationMappingMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.nih.NihValidationMapping"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="businessRulesClass" value="org.kuali.coeus.propdev.impl.s2s.NihValidationMappingRule" />
    <property name="maintainableSections">
      <list>
        <ref bean="NihValidationMappingMaintenanceDocument-Edit"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>id</value>
      </list>
    </property>
	<property name="allowsRecordDeletion" value="true" />

    <property name="documentTypeName" value="NihValidationMappingMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    <property name="webScriptFiles">
        <list>
            <value>../dwr/interface/PageSectionService.js</value>
        </list>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="NihValidationMappingMaintenanceDocument-Edit" parent="NihValidationMappingMaintenanceDocument-Edit-parentBean"/>
  <bean id="NihValidationMappingMaintenanceDocument-Edit-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit NIH Validation Mapping Maintenance Document"/>
    <property name="title" value="Edit NIH Validation Mapping Maintenance Document"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="ruleNumber" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="customMessage"/>
        <bean parent="MaintainableFieldDefinition" p:name="pageId" p:webUILeaveFieldFunction="updateSectionFromPage"/>
        <bean parent="MaintainableFieldDefinition" p:name="sectionId"/>
        <bean parent="MaintainableFieldDefinition" p:name="active"/>
        <bean parent="MaintainableFieldDefinition" p:name="appendToOriginal"/>
        <bean parent="MaintainableFieldDefinition" p:name="forceError"/>
      </list>
    </property>
  </bean>
</beans>
