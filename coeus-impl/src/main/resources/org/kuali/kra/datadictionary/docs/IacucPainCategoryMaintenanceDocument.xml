<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="IacucPainCategoryMaintenanceDocument" parent="IacucPainCategoryMaintenanceDocument-parentBean"/>
	<bean id="IacucPainCategoryMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.IacucPainCategory"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="IacucPainCategoryMaintenanceDocument-EditIacucPainCategory"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>painCategoryCode</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
		<property name="documentTypeName" value="IacucPainCategoryMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="IacucPainCategoryMaintenanceDocument-EditIacucPainCategory" parent="IacucPainCategoryMaintenanceDocument-EditIacucPainCategory-parentBean"/>
	<bean id="IacucPainCategoryMaintenanceDocument-EditIacucPainCategory-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Pain Category"/>
		<property name="title" value="Edit Pain Category"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="painCategoryCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="painCategory" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="painLevel" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
				<bean parent="MaintainableFieldDefinition" p:name="active" p:required="true"/>
			</list>
		</property>
	</bean>
</beans>
