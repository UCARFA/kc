 <beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="AwardBasisOfPaymentMaintenanceDocument" parent="AwardBasisOfPaymentMaintenanceDocument-parentBean"/>
	<bean id="AwardBasisOfPaymentMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.award.home.AwardBasisOfPayment"/>
		<!-- SJW <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/> -->
		<property name="maintainableClass" value="edu.ucar.fanda.kuali.kra.maintenance.UcarAwardBasisOfPaymentMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="AwardBasisOfPaymentMaintenanceDocument-EditAwardBasisOfPayment"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>basisOfPaymentCode</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
	
		
		<property name="documentTypeName" value="AwardBasisOfPaymentMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="AwardBasisOfPaymentMaintenanceDocument-EditAwardBasisOfPayment" parent="AwardBasisOfPaymentMaintenanceDocument-EditAwardBasisOfPayment-parentBean"/>
	<bean id="AwardBasisOfPaymentMaintenanceDocument-EditAwardBasisOfPayment-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Basis Of Payment"/>
		<property name="title" value="Edit Basis Of Payment"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="basisOfPaymentCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
